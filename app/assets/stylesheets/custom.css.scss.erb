/*this is bootstrap hack for the non first child having extra margin*/
/*Divs*/
.row-fluid div.span12 { margin-left : 0px; }
.row-fluid div.span6:nth-child(2n + 3) { margin-left : 0px; }
.row-fluid div.span5:nth-child(2n + 3) { margin-left : 0px; }
.row-fluid div.span4:nth-child(3n + 4) { margin-left : 0px; }
.row-fluid div.span3:nth-child(4n + 5) { margin-left : 0px; }

/*article*/
.row-fluid article.span12 { margin-left : 0px; }
.row-fluid article.span6:nth-child(2n + 3) { margin-left : 0px; }
.row-fluid article.span4:nth-child(3n + 4) { margin-left : 0px; }
.row-fluid article.span3:nth-child(4n + 5) { margin-left : 0px; }

